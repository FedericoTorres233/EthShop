generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Usuario {
  id         Int     @id @default(autoincrement())
  nombre     String
  apellido   String
  direccion  String
  email      String  @unique
  avatar_url String
}

model Categoria {
  id        Int        @id @default(autoincrement())
  nombre    String
}
